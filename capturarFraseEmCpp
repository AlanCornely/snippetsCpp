#include <iostream>
#include <cstring>

int main() {
    
    char frase[20];

    
    std::cout << "Digite uma frase (maximo de 20 caracteres): ";
    std::cin.getline(frase, 20); // Usando cin.getline() para ler a frase com espaÃ§os

    std::cout << "A frase inserida foi: " << frase << std::endl;

    return 0;
}
